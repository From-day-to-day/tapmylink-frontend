{
  "name": "tapmylink-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run pre-build && vite build",
    "preview": "vite preview",
    "pre-build": "tsc -b && eslint --fix ./src && stylelint ./src/**/*.css --fix && prettier --write ./src",
    "prepare": "git config core.hooksPath .git-hooks || echo 'Not in a git repo'; chmod +x .git-hooks/*"
  },
  "dependencies": {
    "classcat": "^5.0.5",
    "preact": "^10.25.3",
    "react-icons": "^5.4.0",
    "swr": "^2.3.0",
    "wouter-preact": "^3.3.5"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.9.3",
    "@types/node": "^22.10.2",
    "eslint": "^9.17.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.3",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "stylelint": "^16.12.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-order": "^6.0.4",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5"
  },
  "browserslist": [
    "defaults and fully supports es6-module",
    "maintained node versions"
  ]
}
